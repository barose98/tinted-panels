<?xml version="1.0" encoding="UTF-8"?>
<subpanel>
<items>
  <button brief="P">
    <name>ctlc</name>
    <category>panel</category>
<icon>&CONTRAST;/actions/edit-copy.svg</icon>
    <command>bash -c 'xdotool key --clearmodifiers ctrl+c' </command>
  </button>
  <button brief="P">
    <name>ctlx</name>
    <category>panel</category>
<icon>&CONTRAST;/actions/edit-cut.svg</icon>
    <command>bash -c 'xdotool key --clearmodifiers ctrl+x' </command>
  </button>
  <button brief="P">
    <name>ctlv</name>
    <category>panel</category>
<icon>&CONTRAST;/actions/edit-paste.svg</icon>
    <command>bash -c 'xdotool key --clearmodifiers ctrl+v' </command>
  </button>
  <button brief="P">
    <name>dl</name>
    <category>panel</category>
<!--    <display>DL</display>-->
<icon>&CONTRAST;/actions/object-rotate-right.svg</icon>
    <command>bash -c 'xdotool key --clearmodifiers Home shift+End ctrl+c End Return ctrl+v'</command>
  </button>

</items>
</subpanel>
